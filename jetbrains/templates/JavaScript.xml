<templateSet group="JavaScript">
  <template name="log" value="console.log($END$)" description="console log" toReformat="false" toShortenFQNames="true">
    <context />
  </template>
  <template name="af" value="const $name$ = ($PARAMS$) =&gt; {&#10;    $END$;&#10;};" description="arrow function definition" toReformat="true" toShortenFQNames="true">
    <variable name="name" expression="jsMethodName()" defaultValue="name" alwaysStopAt="true" />
    <variable name="PARAMS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>